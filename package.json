{
    "name": "vscode-fluentd",
    "displayName": "vscode-fluentd",
    "description": "Fluentd configuration file support for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "msysyamamoto",
    "icon": "https://raw.githubusercontent.com/msysyamamoto/vscode-fluentd/master/images/Fluentd_icon.png",
    "license": "SEE LICENSE IN LICENSE",
    "homepage": "https://raw.githubusercontent.com/msysyamamoto/vscode-fluentd/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/msysyamamoto/vscode-fluentd"
    },
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fluentd",
            "aliases": ["fluentd", "fluentd"],
            "extensions": ["${config:fluentd.extensions}"],
            "configuration": "./language-configuration.json"
        }],
        "configuration": {
			"type": "object",
			"title": "Fluentd Configuration",
			"properties": {
				"fluentd.extensions": {
					"type": "string",
					"default": ".conf",
					"description": "Fluentd extensions. Default is `.conf`"
				}
            }
        },
        "grammars": [{
            "language": "fluentd",
            "scopeName": "source.dig",
            "path": "./syntaxes/fluentd.tmLanguage.json"
        }]
    }
}